{
  "name": "templatebuilder365",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "npm run build:dev && vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "deploy:stage": "npm run build && aws s3 sync dist/ s3://tb365-frontend-stage/ --delete",
    "deploy:prod": "npm run build && aws s3 sync dist/ s3://tb365-frontend-prod/ --delete",
    "// NOTE": "Frontend uses 'stage' for development, Backend uses 'dev' - this is intentional project naming",
    "build:dev": "node scripts/replace-variables.js development",
    "build:stage": "node scripts/replace-variables.js stage",
    "build:prod": "node scripts/replace-variables.js production",
    "restore": "node scripts/restore-templates.js",
    "clean": "node scripts/cleanup-ports.cjs",
    "start": "node scripts/dev-start.cjs"
  },
  "dependencies": {
    "@aws-sdk/client-cognito-identity-provider": "^3.896.0",
    "@aws-sdk/client-s3": "^3.890.0",
    "@aws-sdk/credential-providers": "^3.890.0",
    "immer": "^10.1.1",
    "konva": "^9.3.22",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-konva": "^19.0.7",
    "react-konva-utils": "^1.1.3",
    "uuid": "^13.0.0",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "@vitejs/plugin-react": "^5.0.0",
    "eslint": "^9.33.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "form-data": "^4.0.4",
    "globals": "^16.3.0",
    "node-fetch": "^3.3.2",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.39.1",
    "vite": "^7.1.2"
  }
}
